{
  "id": "${artifactId}-${version}",
  "name": "Pubsub",
  "requires": [],
  "provides": [
    {
      "id": "pub-sub",
      "version": "0.1",
      "handlers": [
        {
          "methods": ["GET"],
          "pathPattern": "/pubsub/events/{id}"
        },
        {
          "methods": ["GET"],
          "pathPattern": "/pubsub/module-info"
        }
      ]
    },
    {
      "id": "_tenant",
      "version": "1.2",
      "interfaceType": "system",
      "handlers": [
        {
          "methods": [
            "POST"
          ],
          "pathPattern": "/_/tenant"
        },
        {
          "methods": [
            "DELETE"
          ],
          "pathPattern": "/_/tenant"
        }
      ]
    }
  ],
  "launchDescriptor": {
    "dockerImage": "${artifactId}:${version}",
    "dockerArgs": {
      "HostConfig": { "PortBindings": { "8081/tcp":  [{ "HostPort": "%p" }] } }
    },
    "dockerPull" : false
  }
}
