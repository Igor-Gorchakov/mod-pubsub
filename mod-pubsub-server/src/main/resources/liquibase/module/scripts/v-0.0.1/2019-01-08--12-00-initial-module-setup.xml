<databaseChangeLog xmlns='http://www.liquibase.org/xml/ns/dbchangelog'
                   xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
                   xsi:schemaLocation='http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd'>

  <changeSet id="create-module_events-table-2019-02-08--13-00" author="Igor-Gorchakov">
    <createTable tableName="module_events">
      <column name="module" type="text">
        <constraints primaryKey="true" nullable="false" unique="true" />
      </column>
      <column name="events" type="text"/>
    </createTable>
  </changeSet>

  <changeSet id="insert-data-to-module_events_table-2019-02-08--14-00" author="Igor-Gorchakov">
    <insert tableName="module_events">
      <column name="module" value="test"/>
      <column name="events" value="list of events"/>
    </insert>
  </changeSet>

</databaseChangeLog>
